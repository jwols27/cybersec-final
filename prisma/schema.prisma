generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id          Int      @id @default(autoincrement())
  email       String   @unique
  password    String?
  secret      String?
  verified2FA Boolean  @default(false)
  googleId    String?  @unique
  createdAt   DateTime @default(now())
}
